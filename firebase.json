{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/kirai1/**",
        "destination": "/kirai1/index.html"
      },
      {
        "source": "/kirai2/**",
        "destination": "/kirai2/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
